    "from src.visualization.visualize import (",
    "    plot_histograms, plot_boxplots, plot_correlation_heatmap, plot_scatter_matrix,\n",
    "    plot_feature_importance, plot_model_performance, plot_prediction_distribution,\n",
    "    plot_residuals, plot_feature_distribution_by_target\n",
    ")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 2. 加载配置文件"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 加载配置文件\n",
    "with open('../configs/config.yaml', 'r', encoding='utf-8') as f:\n",
    "    config = yaml.safe_load(f)\n",
    "\n",
    "# 打印配置信息\n",
    "print(\"配置文件加载成功！\")\n",
    "print(f\"目标变量: {config['data']['target_column']}\")\n",
    "print(f\"数据文件格式: {config['data']['file_extension']}\")\n",
    "print(f\"特征缩放方法: {config['feature_engineering']['scaling']['method']}\")\n",
    "print(f\"模型列表: {', '.join(config['models']['model_list'])}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 3. 数据处理"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 3.1 加载原始数据\n",
    "# 注意：请将原始数据文件放在 data/raw/ 目录下\n",
    "raw_data_path = '../data/raw/sample_data.csv'  # 修改为实际数据文件名\n",
    "\n",
    "try:\n",
    "    data = load_data(raw_data_path, file_format='csv')\n",
    "    print(f\"数据加载成功！形状: {data.shape}\")\n",
    "    print(f\"数据列名: {list(data.columns)}\")\n",
    "except FileNotFoundError:\n",
    "    print(f\"错误：找不到文件 {raw_data_path}\")\n",
    "    print(\"请将原始数据文件放在 data/raw/ 目录下\")\n",
    "    # 使用示例数据作为备选\n",
    "    from sklearn.datasets import load_boston\n",
    "    boston = load_boston()\n",
    "    data = pd.DataFrame(boston.data, columns=boston.feature_names)\n",
    "    data['target'] = boston.target\n",
    "    print(f\"使用示例数据！形状: {data.shape}\")\n",
    "    print(f\"数据列名: {list(data.columns)}\")\n",
    "\n",
    "# 3.2 数据清洗\n",
    "cleaned_data = clean_data(data)\n",
    "\n",
    "# 3.3 数据转换\n",
    "transformed_data = transform_data(cleaned_data)\n",
    "\n",
    "# 3.4 数据划分\n",
    "X_train, X_test, y_train, y_test = split_data(transformed_data, target_column='target')  # 修改为实际目标列名\n",
    "print(f\"训练集形状: {X_train.shape}, {y_train.shape}\")\n",
    "print(f\"测试集形状: {X_test.shape}, {y_test.shape}\")\n",
    "\n",
    "# 3.5 保存处理后的数据\n",
    "save_data(X_train, X_test, y_train, y_test, output_path='../data/processed/')"
   ]
  },
  {
