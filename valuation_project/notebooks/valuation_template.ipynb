    "model_type = 'random_forest'  # 可选: random_forest, gradient_boosting, linear_regression, ridge, lasso\n",
    "\n",
    "# 5.2 训练模型\n",
    "model, best_params = train_model(",
    "    X_train_selected, y_train,\n",
    "    model_type=model_type,\n",
    "    tune_hyperparameters=True\n",
    ")\n",
    "\n",
    "# 5.3 评估模型\n",
    "metrics = evaluate_model(model, X_test_selected, y_test)\n",
    "print(f\"模型评估指标: {metrics}\")\n",
    "\n",
    "# 5.4 保存模型\n",
    "model_path = save_model(model, model_type, best_params)\n",
    "print(f\"模型保存路径: {model_path}\")"
   ]
