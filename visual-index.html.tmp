                                             Math.pow(features.area / 100, areaWeight) * 
                                             Math.pow(1 + (2025 - features.year) * 0.01, yearWeight) * 
                                             Math.pow(1 + features.rooms * 0.1, roomsWeight) *
                                             Math.pow(1 + features.amenityScore, amenityWeight) *
                                             Math.pow(1 + features.developmentPotential * 0.2, developmentWeight) *
                                             Math.pow(1 + features.decorationLevel * 0.2, decorationWeight) *
                                             Math.pow(1 + features.trafficScore * 0.2, trafficWeight) *
                                             Math.pow(1 + features.educationScore * 0.15, educationWeight) *
                                             Math.pow(1 + features.medicalScore * 0.15, medicalWeight) *
                                             Math.pow(1 + features.commercialScore * 0.1, commercialWeight) *
                                             Math.pow(1 + features.greenRate * 0.1, greenWeight) *
                                             Math.pow(1 + features.propertyManagement * 0.1, propertyWeight) *
                                             Math.pow(1 + features.structureScore * 0.1, structureWeight) *
                                             Math.pow(1 + features.viewScore * 0.1, viewWeight);
                        
                        // æ¨¡æ‹Ÿå¸‚åœºå‚è€ƒä»·æ ?                        const marketPrice = basePrice * (1 + features.locationScore * 0.5) * (features.area / 100) * 
                                         (1 + (2025 - features.year) * 0.02) * (1 + features.rooms * 0.1) *
                                         (1 + features.amenityScore * 0.3);
                        
                        // å¤šç›®æ ‡ä¼˜åŒ–ï¼šä»·æ ¼å‡†ç¡®æ€?+ åˆç†çš„æƒé‡åˆ†å¸?                        const accuracyFitness = 1 / (Math.abs(predictedPrice - marketPrice) + 1);
                        const weightDistributionFitness = 1 / (Math.abs(genes.reduce((sum, g) => sum + g, 0) - geneCount / 2) + 1);
                        
                        return accuracyFitness * 0.8 + weightDistributionFitness * 0.2;
                    }
                    
                    // åˆ›å»ºæ›´å¤§çš„ç§ç¾¤ï¼Œä½¿ç”¨æ›´å¤æ‚çš„é—ä¼ ç®—æ³•
                    const population = new GeneticAlgorithm.Population(100, geneCount, fitnessFunction);
                    
                    // è¿›åŒ–æ›´å¤šä»?                    for (let i = 0; i < 200; i++) {
                        population.calculateFitness();
