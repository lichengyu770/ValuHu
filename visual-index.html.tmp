                // è¡¨å•éªŒè¯è§„åˆ™
                ValidationRules: {
                    required: (value) => {
                        if (typeof value === 'string') {
                            return value.trim() !== '';
                        }
                        return value !== undefined && value !== null;
                    },
                    
                    email: (value) => {
                        const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
