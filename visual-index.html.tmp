                    case 'elastic':
                        return this.elastic(t);
                    default:
                        return t;
                }
            },
            
            // ç¼“åŠ¨å‡½æ•°å®ç°
            easeInQuad: function(t) { return t * t; },
            easeOutQuad: function(t) { return t * (2 - t); },
            easeInOutQuad: function(t) { return t < 0.5 ? 2 * t * t : -1 + (4 - 2 * t) * t; },
            easeInCubic: function(t) { return t * t * t; },
            easeOutCubic: function(t) { return (--t) * t * t + 1; },
            easeInOutCubic: function(t) { return t < 0.5 ? 4 * t * t * t : (t - 1) * (2 * t - 2) * (2 * t - 2) + 1; },
            easeInQuart: function(t) { return t * t * t * t; },
            easeOutQuart: function(t) { return 1 - (--t) * t * t * t; },
            easeInOutQuart: function(t) { return t < 0.5 ? 8 * t * t * t * t : 1 - 8 * (--t) * t * t * t; },
            easeInQuint: function(t) { return t * t * t * t * t; },
            easeOutQuint: function(t) { return 1 + (--t) * t * t * t * t; },
            easeInOutQuint: function(t) { return t < 0.5 ? 16 * t * t * t * t * t : 1 + 16 * (--t) * t * t * t * t; },
            
            bounce: function(t) {
                if (t < 1 / 2.75) {
                    return 7.5625 * t * t;
                } else if (t < 2 / 2.75) {
                    return 7.5625 * (t -= 1.5 / 2.75) * t + 0.75;
                } else if (t < 2.5 / 2.75) {
                    return 7.5625 * (t -= 2.25 / 2.75) * t + 0.9375;
                } else {
                    return 7.5625 * (t -= 2.625 / 2.75) * t + 0.984375;
                }
            },
            
            elastic: function(t) {
                return t === 0 || t === 1 ? t : -Math.pow(2, 8 * (t - 1)) * Math.sin((80 * (t - 1) - 7.5) * Math.PI / 15);
            }
        };

        // å¤æ‚çš„è¡¨å•å¤„ç†ç³»ç»?        const FormHandler = {
            forms: {},
            
            // æ³¨å†Œè¡¨å•
            registerForm: function(formId, options) {
                const form = DOMUtils.query(formId);
                if (!form) return;
                
                this.forms[formId] = {
                    form: form,
                    options: options,
                    fields: {},
                    isSubmitting: false
                };
                
                // åˆå§‹åŒ–å­—æ®?                const inputs = DOMUtils.queryAll('input, select, textarea', form);
                inputs.forEach(input => {
                    this.forms[formId].fields[input.name] = {
                        element: input,
                        value: input.value,
                        errors: []
                    };
                    
                    // ç»‘å®šäº‹ä»¶
                    input.addEventListener('input', this.handleInput.bind(this, formId, input.name));
                    input.addEventListener('blur', this.handleBlur.bind(this, formId, input.name));
                    input.addEventListener('change', this.handleChange.bind(this, formId, input.name));
                });
                
                // ç»‘å®šæäº¤äº‹ä»¶
                form.addEventListener('submit', this.handleSubmit.bind(this, formId));
                
                return this;
            },
            
            // å¤„ç†è¾“å…¥äº‹ä»¶
            handleInput: function(formId, fieldName, event) {
                const form = this.forms[formId];
                const field = form.fields[fieldName];
                field.value = event.target.value;
                
                if (form.options.onInput) {
                    form.options.onInput(fieldName, field.value, form);
                }
            },
            
            // å¤„ç†å¤±å»ç„¦ç‚¹äº‹ä»¶
            handleBlur: function(formId, fieldName, event) {
                const form = this.forms[formId];
                const field = form.fields[fieldName];
                
                if (form.options.onBlur) {
                    form.options.onBlur(fieldName, field.value, form);
                }
                
                this.validateField(formId, fieldName);
            },
            
            // å¤„ç†å˜åŒ–äº‹ä»¶
            handleChange: function(formId, fieldName, event) {
                const form = this.forms[formId];
                const field = form.fields[fieldName];
                field.value = event.target.value;
                
                if (form.options.onChange) {
                    form.options.onChange(fieldName, field.value, form);
                }
            },
            
            // éªŒè¯å­—æ®µ
            validateField: function(formId, fieldName) {
                const form = this.forms[formId];
                const field = form.fields[fieldName];
                const schema = form.options.schema;
                
                if (!schema || !schema[fieldName]) {
                    field.errors = [];
                    return true;
                }
                
                const validationResult = DataProcessor.validateData(
                    { [fieldName]: field.value },
                    { [fieldName]: schema[fieldName] }
                );
                
                field.errors = validationResult.errors;
                this.updateFieldErrors(formId, fieldName);
                
                return validationResult.valid;
            },
            
            // æ›´æ–°å­—æ®µé”™è¯¯
            updateFieldErrors: function(formId, fieldName) {
                const form = this.forms[formId];
                const field = form.fields[fieldName];
                const errorElement = DOMUtils.query(`[data-error-for="${fieldName}"]`, form.form);
                
                if (errorElement) {
                    if (field.errors.length > 0) {
                        errorElement.textContent = field.errors[0];
                        errorElement.style.display = 'block';
                        DOMUtils.addClass(field.element, 'error');
                    } else {
                        errorElement.textContent = '';
                        errorElement.style.display = 'none';
                        DOMUtils.removeClass(field.element, 'error');
                    }
                }
            },
            
            // éªŒè¯è¡¨å•
            validateForm: function(formId) {
                const form = this.forms[formId];
                let isValid = true;
                
                for (const fieldName in form.fields) {
                    if (form.fields.hasOwnProperty(fieldName)) {
                        if (!this.validateField(formId, fieldName)) {
                            isValid = false;
                        }
                    }
                }
                
                return isValid;
            },
            
            // å¤„ç†è¡¨å•æäº¤
            handleSubmit: function(formId, event) {
                event.preventDefault();
                
                const form = this.forms[formId];
                if (form.isSubmitting) return;
                
                form.isSubmitting = true;
                
                if (form.options.onSubmitStart) {
                    form.options.onSubmitStart(form);
                }
                
                if (this.validateForm(formId)) {
                    // è·å–è¡¨å•æ•°æ®
                    const formData = {};
                    for (const fieldName in form.fields) {
                        if (form.fields.hasOwnProperty(fieldName)) {
                            formData[fieldName] = form.fields[fieldName].value;
                        }
                    }
                    
                    // è½¬æ¢æ•°æ®
                    if (form.options.transformers) {
                        formData = DataProcessor.transformData(formData, form.options.transformers);
                    }
                    
                    // æäº¤æ•°æ®
                    if (form.options.onSubmit) {
                        form.options.onSubmit(formData, form)
                            .then(response => {
                                if (form.options.onSubmitSuccess) {
                                    form.options.onSubmitSuccess(response, form);
                                }
                            })
                            .catch(error => {
                                if (form.options.onSubmitError) {
                                    form.options.onSubmitError(error, form);
                                }
                            })
                            .finally(() => {
                                form.isSubmitting = false;
                                if (form.options.onSubmitEnd) {
                                    form.options.onSubmitEnd(form);
                                }
                            });
                    } else {
                        form.isSubmitting = false;
                        if (form.options.onSubmitEnd) {
                            form.options.onSubmitEnd(form);
                        }
                    }
                } else {
                    form.isSubmitting = false;
                    if (form.options.onSubmitInvalid) {
                        form.options.onSubmitInvalid(form);
                    }
                }
            },
            
            // è·å–è¡¨å•æ•°æ®
            getFormData: function(formId) {
                const form = this.forms[formId];
                const formData = {};
                
                for (const fieldName in form.fields) {
                    if (form.fields.hasOwnProperty(fieldName)) {
