                // ç»Ÿä¸€çš„æ¨¡æ€æ¡†åˆ›å»ºæ–¹æ³•
                createModal(title, content, options = {}) {
                    const modal = document.createElement('div');
                    modal.style.cssText = `
                        position: fixed;
