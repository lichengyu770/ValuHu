        // å¤æ‚çš„åŠ¨ç”»ç³»ç»?        const AnimationSystem = {
            animations: [],
            requestId: null,
            
            // åˆ›å»ºåŠ¨ç”»
            animate: function(options) {
                const animation = {
                    element: options.element,
                    from: options.from || {},
                    to: options.to || {},
                    duration: options.duration || 1000,
                    easing: options.easing || 'ease',
                    delay: options.delay || 0,
                    onStart: options.onStart || function() {},
                    onUpdate: options.onUpdate || function() {},
                    onComplete: options.onComplete || function() {},
                    startTime: null,
                    progress: 0,
                    isPlaying: false
                };
                
                // è®¡ç®—å±æ€§å˜åŒ–èŒƒå›?                animation.properties = {};
                for (const key in animation.to) {
                    if (animation.to.hasOwnProperty(key)) {
                        const fromValue = parseFloat(animation.from[key] || DOMUtils.getCss(animation.element, key)) || 0;
                        const toValue = parseFloat(animation.to[key]);
                        animation.properties[key] = {
                            from: fromValue,
                            to: toValue,
                            delta: toValue - fromValue
                        };
                    }
                }
                
                this.animations.push(animation);
                
                if (!this.requestId) {
                    this.requestId = requestAnimationFrame(this.loop.bind(this));
                }
                
                return animation;
            },
            
            // åŠ¨ç”»å¾ªç¯
            loop: function(timestamp) {
                const now = timestamp || Date.now();
                let activeAnimations = 0;
                
                for (let i = 0; i < this.animations.length; i++) {
                    const animation = this.animations[i];
                    
                    if (!animation.isPlaying) {
                        if (animation.delay <= 0) {
                            animation.isPlaying = true;
                            animation.startTime = now;
                            animation.onStart(animation);
                        } else {
                            animation.delay -= now - (this.lastTime || now);
                            activeAnimations++;
                            continue;
                        }
                    }
                    
                    const elapsed = now - animation.startTime;
                    if (elapsed < animation.duration) {
                        animation.progress = this.ease(elapsed / animation.duration, animation.easing);
                        this.updateAnimation(animation);
                        activeAnimations++;
                    } else {
                        animation.progress = 1;
                        this.updateAnimation(animation);
                        animation.onComplete(animation);
                        this.animations.splice(i, 1);
                        i--;
                    }
                }
                
                this.lastTime = now;
                
                if (activeAnimations > 0) {
                    this.requestId = requestAnimationFrame(this.loop.bind(this));
                } else {
                    this.requestId = null;
                    this.lastTime = null;
                }
            },
            
            // æ›´æ–°åŠ¨ç”»
            updateAnimation: function(animation) {
                for (const key in animation.properties) {
                    if (animation.properties.hasOwnProperty(key)) {
                        const prop = animation.properties[key];
                        const value = prop.from + prop.delta * animation.progress;
                        const unit = typeof animation.to[key] === 'string' ? animation.to[key].replace(/[0-9.]/g, '') : '';
                        DOMUtils.css(animation.element, {
                            [key]: value + unit
                        });
                    }
                }
                animation.onUpdate(animation);
            },
            
            // ç¼“åŠ¨å‡½æ•°
            ease: function(t, easing) {
                switch (easing) {
                    case 'linear':
                        return t;
                    case 'ease':
                        return this.easeInOutCubic(t);
                    case 'ease-in':
                        return this.easeInCubic(t);
                    case 'ease-out':
                        return this.easeOutCubic(t);
                    case 'ease-in-out':
                        return this.easeInOutCubic(t);
                    case 'ease-in-quad':
                        return this.easeInQuad(t);
                    case 'ease-out-quad':
                        return this.easeOutQuad(t);
                    case 'ease-in-out-quad':
                        return this.easeInOutQuad(t);
                    case 'ease-in-cubic':
                        return this.easeInCubic(t);
                    case 'ease-out-cubic':
                        return this.easeOutCubic(t);
                    case 'ease-in-out-cubic':
                        return this.easeInOutCubic(t);
                    case 'ease-in-quart':
                        return this.easeInQuart(t);
                    case 'ease-out-quart':
                        return this.easeOutQuart(t);
                    case 'ease-in-out-quart':
                        return this.easeInOutQuart(t);
                    case 'ease-in-quint':
                        return this.easeInQuint(t);
                    case 'ease-out-quint':
                        return this.easeOutQuint(t);
                    case 'ease-in-out-quint':
                        return this.easeInOutQuint(t);
                    case 'bounce':
