            // æ‡’åŠ è½½å‡½æ•?            lazyLoad: function(selector = '.lazy-load', options = {}) {
                const { rootMargin = '0px', threshold = 0.1 } = options;
                
                const observer = new IntersectionObserver(function(entries) {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            const target = entry.target;
                            if (target.tagName === 'IMG') {
                                if (target.dataset.src) {
                                    target.src = target.dataset.src;
                                    target.removeAttribute('data-src');
                                }
                            } else if (target.dataset.background) {
                                target.style.backgroundImage = `url(${target.dataset.background})`;
                                target.removeAttribute('data-background');
                            }
                            observer.unobserve(target);
                        }
                    });
                }, { rootMargin, threshold });
                
                document.querySelectorAll(selector).forEach(el => observer.observe(el));
                return observer;
            },
            
            // ç¼“å­˜å‡½æ•°
            cache: function(func, options = {}) {
                const { maxSize = 100, ttl = null } = options;
                const cache = new Map();
                const cacheMeta = new Map();
                
                return function(...args) {
                    const key = JSON.stringify(args);
                    
                    // æ£€æŸ¥ç¼“å­˜æ˜¯å¦å­˜åœ¨ä¸”æœªè¿‡æœ?                    if (cache.has(key)) {
                        const meta = cacheMeta.get(key);
                        if (!ttl || Date.now() - meta.timestamp < ttl) {
                            return cache.get(key);
                        }
                        // è¿‡æœŸåˆ™åˆ é™?                        cache.delete(key);
                        cacheMeta.delete(key);
                    }
                    
                    // æ‰§è¡Œå‡½æ•°å¹¶ç¼“å­˜ç»“æ?                    const result = func.apply(this, args);
                    cache.set(key, result);
