        const Security = {
            // åŠ å¯†æ¨¡å—
            encryption: {
                // ç®€å•çš„å‡¯æ’’å¯†ç 
                caesarCipher: function(text, shift = 3, encrypt = true) {
                    const alphabet = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ';
                    let result = '';
                    
                    for (let i = 0; i < text.length; i++) {
                        const char = text[i];
                        const index = alphabet.indexOf(char);
                        
                        if (index === -1) {
                            result += char;
                        } else {
                            const direction = encrypt ? 1 : -1;
                            const newIndex = (index + direction * shift + alphabet.length) % alphabet.length;
                            result += alphabet[newIndex];
                        }
                    }
                    
                    return result;
                },
                
                // ç®€å•çš„å“ˆå¸Œå‡½æ•°
                simpleHash: function(text) {
                    let hash = 0;
                    for (let i = 0; i < text.length; i++) {
                        const char = text.charCodeAt(i);
                        hash = ((hash << 5) - hash) + char;
                        hash = hash & hash;
                    }
                    return hash;
                },
                
                // ç”Ÿæˆéšæœºå¯†é’¥
                generateRandomKey: function(length = 32) {
                    const chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789!@#$%^&*()_+-=[]{}|;:,.<>?';
                    let key = '';
                    for (let i = 0; i < length; i++) {
                        key += chars.charAt(Math.floor(Math.random() * chars.length));
                    }
                    return key;
                }
