                }
                
                return formData;
            },
            
            // è®¾ç½®è¡¨å•æ•°æ®
            setFormData: function(formId, data) {
                const form = this.forms[formId];
                
                for (const fieldName in data) {
                    if (data.hasOwnProperty(fieldName) && form.fields[fieldName]) {
                        const field = form.fields[fieldName];
                        field.value = data[fieldName];
                        field.element.value = data[fieldName];
                    }
                }
                
                return this;
            },
            
            // é‡ç½®è¡¨å•
            resetForm: function(formId) {
                const form = this.forms[formId];
                form.form.reset();
                
                for (const fieldName in form.fields) {
                    if (form.fields.hasOwnProperty(fieldName)) {
