        // å¤æ‚çš„æ•°æ®å¤„ç†å‡½æ•?        const DataProcessor = {
            // æ•°æ®éªŒè¯
            validateData: function(data, schema) {
                const errors = [];
                
                for (const key in schema) {
                    if (schema.hasOwnProperty(key)) {
                        const field = schema[key];
                        const value = data[key];
                        
                        // å¿…å¡«å­—æ®µæ£€æŸ?                        if (field.required && (value === undefined || value === null || value === '')) {
                            errors.push(`${key} is required`);
                            continue;
                        }
                        
                        // ç±»å‹æ£€æŸ?                        if (value !== undefined && value !== null && field.type) {
                            const actualType = Array.isArray(value) ? 'array' : typeof value;
                            if (actualType !== field.type && field.type !== 'any') {
                                errors.push(`${key} must be of type ${field.type}`);
                            }
                        }
                        
                        // æœ€å°å€¼æ£€æŸ?                        if (value !== undefined && value !== null && field.min !== undefined) {
                            if (typeof value === 'number' && value < field.min) {
                                errors.push(`${key} must be greater than or equal to ${field.min}`);
