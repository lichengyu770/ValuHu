                                    property.location.toLowerCase().includes(query.toLowerCase()) ||
                                    property.address.toLowerCase().includes(query.toLowerCase()));
                        });
                    }
                    
                    filterProperties(filters) {
                        return this.getAllProperties().filter(property => {
                            for (const key in filters) {
                                if (property[key] !== filters[key]) {
                                    return false;
                                }
                            }
                            return true;
                        });
                    }
                    
                    sortProperties(sortBy, order = 'asc') {
                        const properties = this.getAllProperties();
                        return properties.sort((a, b) => {
                            if (a[sortBy] < b[sortBy]) {
                                return order === 'asc' ? -1 : 1;
                            }
                            if (a[sortBy] > b[sortBy]) {
                                return order === 'asc' ? 1 : -1;
                            }
                            return 0;
                        });
                    }
                    
                    paginateProperties(page, pageSize = 10) {
                        const properties = this.getAllProperties();
                        const startIndex = (page - 1) * pageSize;
                        const endIndex = startIndex + pageSize;
                        return {
                            properties: properties.slice(startIndex, endIndex),
                            total: properties.length,
                            page,
                            pageSize,
                            totalPages: Math.ceil(properties.length / pageSize)
                        };
                    }
