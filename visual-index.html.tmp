                                errors.push(`${key} must be at least ${field.min} characters long`);
                            } else if (Array.isArray(value) && value.length < field.min) {
                                errors.push(`${key} must have at least ${field.min} items`);
                            }
                        }
                        
                        // æœ€å¤§å€¼æ£€æŸ?                        if (value !== undefined && value !== null && field.max !== undefined) {
                            if (typeof value === 'number' && value > field.max) {
                                errors.push(`${key} must be less than or equal to ${field.max}`);
                            } else if (typeof value === 'string' && value.length > field.max) {
                                errors.push(`${key} must be at most ${field.max} characters long`);
                            } else if (Array.isArray(value) && value.length > field.max) {
                                errors.push(`${key} must have at most ${field.max} items`);
                            }
                        }
                        
                        // æ­£åˆ™è¡¨è¾¾å¼æ£€æŸ?                        if (value !== undefined && value !== null && field.pattern) {
                            const regex = new RegExp(field.pattern);
                            if (typeof value === 'string' && !regex.test(value)) {
                                errors.push(`${key} must match pattern ${field.pattern}`);
                            }
                        }
                        
                        // æšä¸¾æ£€æŸ?                        if (value !== undefined && value !== null && field.enum) {
                            if (!field.enum.includes(value)) {
                                errors.push(`${key} must be one of ${field.enum.join(', ')}`);
                            }
