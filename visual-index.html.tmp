                        // è‡ªå®šä¹‰éªŒè¯å‡½æ•?                        if (value !== undefined && value !== null && field.validator) {
                            const validatorResult = field.validator(value, data);
                            if (validatorResult !== true) {
                                errors.push(validatorResult || `${key} is invalid`);
                            }
                        }
                    }
                }
                
                return {
                    valid: errors.length === 0,
                    errors: errors
                };
            },

            // æ•°æ®è½¬æ¢
            transformData: function(data, transformers) {
                const result = { ...data };
                
                for (const key in transformers) {
                    if (transformers.hasOwnProperty(key)) {
                        const transformer = transformers[key];
                        if (result.hasOwnProperty(key)) {
                            result[key] = transformer(result[key], result);
                        }
                    }
                }
                
                return result;
            },

            // æ•°æ®è¿‡æ»¤
            filterData: function(dataArray, filters) {
                return dataArray.filter(item => {
                    for (const key in filters) {
                        if (filters.hasOwnProperty(key)) {
                            const filter = filters[key];
                            const value = item[key];
                            
                            if (typeof filter === 'function') {
                                if (!filter(value, item)) {
                                    return false;
                                }
                            } else if (Array.isArray(filter)) {
                                if (!filter.includes(value)) {
                                    return false;
                                }
                            } else if (filter instanceof RegExp) {
                                if (typeof value === 'string' && !filter.test(value)) {
                                    return false;
                                }
                            } else {
                                if (value !== filter) {
                                    return false;
                                }
                            }
                        }
                    }
                    return true;
                });
            },

            // æ•°æ®æ’åº
            sortData: function(dataArray, sortConfig) {
                const result = [...dataArray];
                
                result.sort((a, b) => {
                    for (const key in sortConfig) {
                        if (sortConfig.hasOwnProperty(key)) {
                            const order = sortConfig[key];
                            let comparison = 0;
                            
                            if (a[key] < b[key]) {
                                comparison = -1;
                            } else if (a[key] > b[key]) {
                                comparison = 1;
                            }
                            
                            if (comparison !== 0) {
                                return order === 'desc' ? -comparison : comparison;
                            }
                        }
                    }
                    return 0;
