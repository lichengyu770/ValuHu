            // é˜¶ä¹˜
            factorial: function(n) {
                if (n === 0 || n === 1) return 1;
                let result = 1;
                for (let i = 2; i <= n; i++) {
                    result *= i;
                }
                return result;
            },

            // ç»„åˆæ•?            combination: function(n, k) {
                if (k > n) return 0;
                if (k === 0 || k === n) return 1;
                k = Math.min(k, n - k);
                let result = 1;
                for (let i = 1; i <= k; i++) {
                    result = result * (n - k + i) / i;
                }
                return Math.round(result);
