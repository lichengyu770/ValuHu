            // æ•°æ®èšåˆ
            aggregateData: function(dataArray, aggregations) {
                const result = {};
                
                for (const key in aggregations) {
                    if (aggregations.hasOwnProperty(key)) {
                        const aggregation = aggregations[key];
                        const { field, func } = aggregation;
                        
                        if (typeof func === 'string') {
                            switch (func.toLowerCase()) {
                                case 'sum':
                                    result[key] = dataArray.reduce((sum, item) => sum + (item[field] || 0), 0);
                                    break;
                                case 'avg':
