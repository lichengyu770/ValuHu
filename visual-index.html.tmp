                    },
                    
                    url: (value) => {
                        const urlRegex = /^(https?:\/\/)?([\da-z.-]+)\.([a-z.]{2,6})([\/\w .-]*)*\/?$/;
                        return urlRegex.test(value);
                    },
                    
                    creditCard: (value) => {
