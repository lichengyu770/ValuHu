                insert(position, element) {
                    if (position < 0 || position > this.length) return false;
                    
                    const node = { element, next: null };
                    
                    if (position === 0) {
                        node.next = this.head;
                        this.head = node;
                        if (!this.tail) {
                            this.tail = node;
                        }
                    } else if (position === this.length) {
                        this.tail.next = node;
                        this.tail = node;
                    } else {
                        let current = this.head;
                        let previous = null;
                        let index = 0;
                        
                        while (index < position) {
                            previous = current;
