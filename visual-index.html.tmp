                    validateForms(formDataArray, rulesArray) {
                        const results = [];
                        
                        for (let i = 0; i < formDataArray.length; i++) {
                            const validator = new FormSystem.Validator(rulesArray[i]);
                            const isValid = validator.validate(formDataArray[i]);
                            results.push({
