            },
            
            // æ·±åº¦ä¼˜å…ˆæœç´¢
            dfs(graph, startVertex, callback) {
                const visited = new Set();
                
                const dfsHelper = (vertex) => {
                    visited.add(vertex);
                    callback(vertex);
                    
                    for (let neighbor of graph.vertices.get(vertex).keys()) {
                        if (!visited.has(neighbor)) {
                            dfsHelper(neighbor);
                        }
                    }
                };
                
                dfsHelper(startVertex);
            },
            
            // å¹¿åº¦ä¼˜å…ˆæœç´¢
            bfs(graph, startVertex, callback) {
                const visited = new Set();
                const queue = [startVertex];
                visited.add(startVertex);
                
                while (queue.length > 0) {
                    const vertex = queue.shift();
                    callback(vertex);
                    
                    for (let neighbor of graph.vertices.get(vertex).keys()) {
                        if (!visited.has(neighbor)) {
                            visited.add(neighbor);
                            queue.push(neighbor);
                        }
                    }
                }
            },
            
            // Dijkstraç®—æ³•ï¼šæœ€çŸ­è·¯å¾?            dijkstra(graph, startVertex) {
                const distances = new Map();
                const previous = new Map();
                const priorityQueue = new GraphAlgorithms.PriorityQueue();
                
                // åˆå§‹åŒ–è·ç¦?                for (let vertex of graph.vertices.keys()) {
                    distances.set(vertex, Infinity);
                    previous.set(vertex, null);
                }
                distances.set(startVertex, 0);
                priorityQueue.enqueue(startVertex, 0);
                
                while (!priorityQueue.isEmpty()) {
                    const current = priorityQueue.dequeue();
                    
                    for (let [neighbor, weight] of graph.vertices.get(current).entries()) {
                        const distance = distances.get(current) + weight;
                        
                        if (distance < distances.get(neighbor)) {
                            distances.set(neighbor, distance);
                            previous.set(neighbor, current);
                            priorityQueue.enqueue(neighbor, distance);
                        }
                    }
                }
                
                return { distances, previous };
            },
            
            // æœ€å°ç”Ÿæˆæ ‘ï¼šPrimç®—æ³•
            prim(graph, startVertex) {
                const mst = new GraphAlgorithms.Graph();
                const visited = new Set();
                const priorityQueue = new GraphAlgorithms.PriorityQueue();
                
                visited.add(startVertex);
                
                // æ·»åŠ èµ·å§‹é¡¶ç‚¹çš„æ‰€æœ‰è¾¹åˆ°ä¼˜å…ˆé˜Ÿåˆ?                for (let [neighbor, weight] of graph.vertices.get(startVertex).entries()) {
                    priorityQueue.enqueue({ source: startVertex, destination: neighbor }, weight);
                }
                
                while (!priorityQueue.isEmpty() && visited.size < graph.vertices.size) {
                    const edge = priorityQueue.dequeue();
                    const { source, destination } = edge;
                    
                    if (!visited.has(destination)) {
                        visited.add(destination);
                        mst.addEdge(source, destination, graph.vertices.get(source).get(destination));
                        
                        // æ·»åŠ æ–°é¡¶ç‚¹çš„æ‰€æœ‰è¾¹åˆ°ä¼˜å…ˆé˜Ÿåˆ?                        for (let [neighbor, weight] of graph.vertices.get(destination).entries()) {
                            if (!visited.has(neighbor)) {
                                priorityQueue.enqueue({ source: destination, neighbor }, weight);
                            }
                        }
                    }
                }
                
                return mst;
            },
            
            // ä¼˜å…ˆé˜Ÿåˆ—å®ç°
            PriorityQueue: class {
                constructor() {
                    this.values = [];
                }
                
                enqueue(element, priority) {
                    this.values.push({ element, priority });
                    this.sort();
                }
                
                dequeue() {
                    return this.values.shift().element;
                }
                
                isEmpty() {
                    return this.values.length === 0;
                }
                
                sort() {
                    this.values.sort((a, b) => a.priority - b.priority);
                }
            }
        };
        
        // æ“ä½œç³»ç»Ÿæ¨¡æ‹Ÿå™?        const OperatingSystemSimulator = {
            // è¿›ç¨‹ç±?            Process: class {
                constructor(pid, arrivalTime, burstTime, priority = 0) {
                    this.pid = pid;
                    this.arrivalTime = arrivalTime;
                    this.burstTime = burstTime;
                    this.remainingTime = burstTime;
                    this.priority = priority;
                    this.startTime = null;
                    this.completionTime = null;
                    this.waitingTime = 0;
                    this.turnaroundTime = 0;
                    this.responseTime = null;
                }
            },
            
            // CPUè°ƒåº¦ç®—æ³•
            Scheduler: class {
                constructor() {
                    this.processes = [];
                    this.currentTime = 0;
                    this.completedProcesses = [];
                }
                
                // æ·»åŠ è¿›ç¨‹
                addProcess(process) {
                    this.processes.push(process);
                }
                
                // FCFSè°ƒåº¦ç®—æ³•
                fcfs() {
                    // æŒ‰åˆ°è¾¾æ—¶é—´æ’åº?                    this.processes.sort((a, b) => a.arrivalTime - b.arrivalTime);
                    
                    for (let process of this.processes) {
                        if (this.currentTime < process.arrivalTime) {
                            this.currentTime = process.arrivalTime;
                        }
                        
                        process.startTime = this.currentTime;
                        process.responseTime = process.startTime - process.arrivalTime;
                        this.currentTime += process.burstTime;
                        process.completionTime = this.currentTime;
                        process.turnaroundTime = process.completionTime - process.arrivalTime;
                        process.waitingTime = process.turnaroundTime - process.burstTime;
                        
                        this.completedProcesses.push(process);
                    }
                    
                    return this.completedProcesses;
                }
                
                // SJFè°ƒåº¦ç®—æ³•
                sjf() {
                    const remainingProcesses = [...this.processes];
                    
                    while (remainingProcesses.length > 0) {
                        // é€‰æ‹©å½“å‰æ—¶é—´å·²åˆ°è¾¾ä¸”å‰©ä½™æ—¶é—´æœ€çŸ­çš„è¿›ç¨‹
                        const availableProcesses = remainingProcesses.filter(p => p.arrivalTime <= this.currentTime);
                        
                        if (availableProcesses.length === 0) {
                            // æ²¡æœ‰å¯ç”¨è¿›ç¨‹ï¼Œæ—¶é—´å‰è¿›åˆ°ä¸‹ä¸€ä¸ªè¿›ç¨‹åˆ°è¾¾æ—¶é—?                            const nextProcess = remainingProcesses.reduce((earliest, p) => 
                                p.arrivalTime < earliest.arrivalTime ? p : earliest);
                            this.currentTime = nextProcess.arrivalTime;
                            continue;
                        }
                        
                        // é€‰æ‹©å‰©ä½™æ—¶é—´æœ€çŸ­çš„è¿›ç¨‹
                        const currentProcess = availableProcesses.reduce((shortest, p) => 
                            p.remainingTime < shortest.remainingTime ? p : shortest);
                        
                        if (currentProcess.responseTime === null) {
                            currentProcess.responseTime = this.currentTime - currentProcess.arrivalTime;
                        }
                        
                        // æ‰§è¡Œä¸€ä¸ªæ—¶é—´ç‰‡
                        this.currentTime++;
                        currentProcess.remainingTime--;
                        
                        if (currentProcess.remainingTime === 0) {
                            currentProcess.completionTime = this.currentTime;
                            currentProcess.turnaroundTime = currentProcess.completionTime - currentProcess.arrivalTime;
                            currentProcess.waitingTime = currentProcess.turnaroundTime - currentProcess.burstTime;
                            this.completedProcesses.push(currentProcess);
                            
                            // ä»å‰©ä½™è¿›ç¨‹ä¸­ç§»é™¤
                            const index = remainingProcesses.indexOf(currentProcess);
                            remainingProcesses.splice(index, 1);
                        }
                    }
                    
                    return this.completedProcesses;
                }
                
                // ä¼˜å…ˆçº§è°ƒåº¦ç®—æ³?                priority() {
                    const remainingProcesses = [...this.processes];
                    
                    while (remainingProcesses.length > 0) {
                        // é€‰æ‹©å½“å‰æ—¶é—´å·²åˆ°è¾¾ä¸”ä¼˜å…ˆçº§æœ€é«˜çš„è¿›ç¨‹
                        const availableProcesses = remainingProcesses.filter(p => p.arrivalTime <= this.currentTime);
                        
                        if (availableProcesses.length === 0) {
                            // æ²¡æœ‰å¯ç”¨è¿›ç¨‹ï¼Œæ—¶é—´å‰è¿›åˆ°ä¸‹ä¸€ä¸ªè¿›ç¨‹åˆ°è¾¾æ—¶é—?                            const nextProcess = remainingProcesses.reduce((earliest, p) => 
                                p.arrivalTime < earliest.arrivalTime ? p : earliest);
                            this.currentTime = nextProcess.arrivalTime;
                            continue;
                        }
                        
                        // é€‰æ‹©ä¼˜å…ˆçº§æœ€é«˜çš„è¿›ç¨‹ï¼ˆæ•°å€¼è¶Šå°ä¼˜å…ˆçº§è¶Šé«˜ï¼?                        const currentProcess = availableProcesses.reduce((highest, p) => 
                            p.priority < highest.priority ? p : highest);
                        
                        if (currentProcess.responseTime === null) {
                            currentProcess.responseTime = this.currentTime - currentProcess.arrivalTime;
                        }
                        
                        // æ‰§è¡Œä¸€ä¸ªæ—¶é—´ç‰‡
                        this.currentTime++;
                        currentProcess.remainingTime--;
                        
                        if (currentProcess.remainingTime === 0) {
                            currentProcess.completionTime = this.currentTime;
                            currentProcess.turnaroundTime = currentProcess.completionTime - currentProcess.arrivalTime;
                            currentProcess.waitingTime = currentProcess.turnaroundTime - currentProcess.burstTime;
                            this.completedProcesses.push(currentProcess);
                            
                            // ä»å‰©ä½™è¿›ç¨‹ä¸­ç§»é™¤
                            const index = remainingProcesses.indexOf(currentProcess);
                            remainingProcesses.splice(index, 1);
                        }
                    }
                    
                    return this.completedProcesses;
                }
            },
            
            // å†…å­˜ç®¡ç†
            MemoryManager: class {
                constructor(totalMemory) {
                    this.totalMemory = totalMemory;
                    this.memoryBlocks = [{ start: 0, size: totalMemory, free: true }];
                }
                
                // é¦–æ¬¡é€‚åº”ç®—æ³•
                firstFit(processId, size) {
                    for (let i = 0; i < this.memoryBlocks.length; i++) {
                        const block = this.memoryBlocks[i];
                        if (block.free && block.size >= size) {
                            // åˆ†é…å†…å­˜
                            const allocatedBlock = { start: block.start, size, free: false, processId };
                            this.memoryBlocks.splice(i, 1);
                            
                            // å¦‚æœæœ‰å‰©ä½™ç©ºé—´ï¼Œåˆ›å»ºæ–°çš„ç©ºé—²å?                            if (block.size > size) {
                                const remainingBlock = { start: block.start + size, size: block.size - size, free: true };
                                this.memoryBlocks.splice(i, 0, remainingBlock);
                            }
                            
                            this.memoryBlocks.splice(i, 0, allocatedBlock);
                            return true;
                        }
                    }
                    return false;
                }
                
                // æœ€ä½³é€‚åº”ç®—æ³•
                bestFit(processId, size) {
                    let bestBlockIndex = -1;
                    let bestBlockSize = Infinity;
                    
                    for (let i = 0; i < this.memoryBlocks.length; i++) {
                        const block = this.memoryBlocks[i];
                        if (block.free && block.size >= size && block.size < bestBlockSize) {
                            bestBlockIndex = i;
                            bestBlockSize = block.size;
                        }
                    }
                    
                    if (bestBlockIndex !== -1) {
                        const block = this.memoryBlocks[bestBlockIndex];
                        const allocatedBlock = { start: block.start, size, free: false, processId };
                        this.memoryBlocks.splice(bestBlockIndex, 1);
                        
                        if (block.size > size) {
                            const remainingBlock = { start: block.start + size, size: block.size - size, free: true };
                            this.memoryBlocks.splice(bestBlockIndex, 0, remainingBlock);
                        }
                        
                        this.memoryBlocks.splice(bestBlockIndex, 0, allocatedBlock);
                        return true;
                    }
                    
                    return false;
                }
                
                // é‡Šæ”¾å†…å­˜
                free(processId) {
                    const allocatedBlocks = this.memoryBlocks.filter(block => block.processId === processId);
                    
                    for (let block of allocatedBlocks) {
                        block.free = true;
                        block.processId = null;
                    }
                    
                    // åˆå¹¶ç›¸é‚»çš„ç©ºé—²å—
                    this.mergeFreeBlocks();
                }
                
                // åˆå¹¶ç©ºé—²å?                mergeFreeBlocks() {
                    let i = 0;
                    while (i < this.memoryBlocks.length - 1) {
                        const current = this.memoryBlocks[i];
                        const next = this.memoryBlocks[i + 1];
                        
                        if (current.free && next.free) {
                            // åˆå¹¶å?                            const mergedBlock = { start: current.start, size: current.size + next.size, free: true };
                            this.memoryBlocks.splice(i, 2, mergedBlock);
                        } else {
                            i++;
                        }
                    }
                }
            }
        };
        
        // æ•°æ®åº“ç³»ç»Ÿæ¨¡æ‹Ÿå™¨
        const DatabaseSystemSimulator = {
            // å…³ç³»å‹æ•°æ®åº“
            RelationalDatabase: class {
                constructor() {
                    this.tables = new Map();
                    this.indexes = new Map();
                }
                
                // åˆ›å»ºè¡?                createTable(tableName, columns) {
                    if (this.tables.has(tableName)) {
                        throw new Error(`Table ${tableName} already exists`);
                    }
                    
                    this.tables.set(tableName, {
                        columns,
                        rows: [],
                        primaryKey: columns.find(col => col.primaryKey)?.name || null
                    });
                }
                
                // æ’å…¥æ•°æ®
                insert(tableName, data) {
                    const table = this.tables.get(tableName);
                    if (!table) {
                        throw new Error(`Table ${tableName} does not exist`);
                    }
                    
                    // éªŒè¯æ•°æ®
                    for (let column of table.columns) {
                        if (column.required && !data.hasOwnProperty(column.name)) {
                            throw new Error(`Column ${column.name} is required`);
                        }
                    }
                    
                    // ç”ŸæˆIDï¼ˆå¦‚æœæœ‰è‡ªå¢ä¸»é”®ï¼?                    const primaryKeyColumn = table.columns.find(col => col.primaryKey);
                    if (primaryKeyColumn && primaryKeyColumn.autoIncrement) {
                        const maxId = table.rows.reduce((max, row) => Math.max(max, row[primaryKeyColumn.name] || 0), 0);
                        data[primaryKeyColumn.name] = maxId + 1;
                    }
                    
                    table.rows.push(data);
                    
                    // æ›´æ–°ç´¢å¼•
                    this.updateIndexes(tableName, data);
                }
                
                // æŸ¥è¯¢æ•°æ®
                select(tableName, conditions = null, columns = '*') {
                    const table = this.tables.get(tableName);
                    if (!table) {
                        throw new Error(`Table ${tableName} does not exist`);
                    }
                    
                    let result = [...table.rows];
                    
                    // åº”ç”¨æ¡ä»¶è¿‡æ»¤
                    if (conditions) {
                        result = result.filter(row => this.evaluateConditions(row, conditions));
                    }
                    
                    // é€‰æ‹©åˆ?                    if (columns !== '*') {
                        result = result.map(row => {
                            const selected = {};
                            for (let col of columns) {
                                selected[col] = row[col];
                            }
                            return selected;
                        });
                    }
                    
                    return result;
                }
                
                // æ›´æ–°æ•°æ®
                update(tableName, data, conditions) {
                    const table = this.tables.get(tableName);
                    if (!table) {
                        throw new Error(`Table ${tableName} does not exist`);
                    }
                    
                    let updatedCount = 0;
                    
                    for (let row of table.rows) {
                        if (this.evaluateConditions(row, conditions)) {
                            // æ›´æ–°è¡Œæ•°æ?                            for (let [key, value] of Object.entries(data)) {
                                row[key] = value;
                            }
                            updatedCount++;
                        }
                    }
                    
                    return updatedCount;
                }
                
                // åˆ é™¤æ•°æ®
                delete(tableName, conditions) {
                    const table = this.tables.get(tableName);
                    if (!table) {
                        throw new Error(`Table ${tableName} does not exist`);
                    }
                    
                    const initialLength = table.rows.length;
                    table.rows = table.rows.filter(row => !this.evaluateConditions(row, conditions));
                    
                    return initialLength - table.rows.length;
                }
                
                // åˆ›å»ºç´¢å¼•
                createIndex(tableName, columnName) {
                    const table = this.tables.get(tableName);
                    if (!table) {
                        throw new Error(`Table ${tableName} does not exist`);
                    }
                    
                    const index = new Map();
                    for (let i = 0; i < table.rows.length; i++) {
                        const value = table.rows[i][columnName];
                        if (!index.has(value)) {
                            index.set(value, []);
                        }
                        index.get(value).push(i);
                    }
                    
                    const indexName = `${tableName}_${columnName}_idx`;
                    this.indexes.set(indexName, {
                        tableName,
                        columnName,
                        index
                    });
                }
                
                // è¯„ä¼°æ¡ä»¶
                evaluateConditions(row, conditions) {
                    for (let [column, condition] of Object.entries(conditions)) {
                        if (typeof condition === 'object') {
                            // å¤æ‚æ¡ä»¶ï¼Œå¦‚ { $gt: 10 }
                            for (let [operator, value] of Object.entries(condition)) {
                                switch (operator) {
                                    case '$eq':
                                        if (row[column] !== value) return false;
                                        break;
                                    case '$ne':
                                        if (row[column] === value) return false;
                                        break;
                                    case '$gt':
                                        if (row[column] <= value) return false;
                                        break;
                                    case '$lt':
                                        if (row[column] >= value) return false;
                                        break;
                                    case '$gte':
                                        if (row[column] < value) return false;
                                        break;
                                    case '$lte':
                                        if (row[column] > value) return false;
                                        break;
                                    case '$in':
                                        if (!value.includes(row[column])) return false;
                                        break;
                                    case '$nin':
                                        if (value.includes(row[column])) return false;
                                        break;
                                }
                            }
                        } else {
                            // ç®€å•æ¡ä»¶ï¼Œå¦?column: value
                            if (row[column] !== condition) {
                                return false;
                            }
                        }
                    }
                    return true;
                }
                
                // æ›´æ–°ç´¢å¼•
                updateIndexes(tableName, data) {
                    for (let [indexName, indexInfo] of this.indexes.entries()) {
                        if (indexInfo.tableName === tableName) {
                            const value = data[indexInfo.columnName];
                            if (!indexInfo.index.has(value)) {
                                indexInfo.index.set(value, []);
                            }
                            indexInfo.index.get(value).push(this.tables.get(tableName).rows.length - 1);
                        }
                    }
                }
            }
        };
        
