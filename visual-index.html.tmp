                    unitPrice: (features) => {
                        // åŸºäºè·¯çº¿ä»·çš„ä¼°ä»·
                        const baseUnitPrice = 18000; // è·¯çº¿ä»?                        const depthFactor = 1 - Math.abs(features.floor - features.totalFloors / 2) / features.totalFloors;
                        const frontageFactor = 1 + features.trafficScore * 0.2;
                        
                        return baseUnitPrice * depthFactor * frontageFactor * features.area;
                    }
