                    });
                    
                    return { modal, closeModal };
                },
                
                // ç»Ÿä¸€çš„æœç´¢åŠŸèƒ?                search(keyword, type = 'all') {
                    console.log(`æœç´¢: ${keyword}, ç±»å‹: ${type}`);
                    // æ·»åŠ åˆ°æœç´¢å†å?                    this.data.searchHistory.unshift({
                        keyword,
                        type,
                        timestamp: new Date().toISOString()
                    });
                    // ä¿æŒæœç´¢å†å²ä¸è¶…è¿?0æ?                    this.data.searchHistory = this.data.searchHistory.slice(0, 20);
                    return this.data.searchHistory;
                },
                
                // ç»Ÿä¸€çš„ç­›é€‰åŠŸèƒ?                filter(data, filters) {
                    return data.filter(item => {
                        for (const [key, value] of Object.entries(filters)) {
                            if (item[key] !== value) return false;
                        }
                        return true;
                    });
                },
                
                // ç»Ÿä¸€çš„åˆ†é¡µåŠŸèƒ?                paginate(data, page = 1, pageSize = 10) {
                    const start = (page - 1) * pageSize;
                    const end = start + pageSize;
                    return {
                        items: data.slice(start, end),
                        total: data.length,
                        page,
                        pageSize,
                        totalPages: Math.ceil(data.length / pageSize)
                    };
                },
                
                // æ·»åŠ åˆ°æ”¶è—?                addToFavorites(item, type) {
                    const existingIndex = this.data.favorites.findIndex(fav => fav.id === item.id && fav.type === type);
                    if (existingIndex === -1) {
                        this.data.favorites.push({
                            ...item,
                            type,
                            addedAt: new Date().toISOString()
                        });
                    }
                    return this.data.favorites;
                },
                
                // ç§»é™¤æ”¶è—
                removeFromFavorites(id, type) {
                    this.data.favorites = this.data.favorites.filter(fav => !(fav.id === id && fav.type === type));
                    return this.data.favorites;
                },
                
                // æ·»åŠ åˆ°æœ€è¿‘æŸ¥çœ?                addToRecentViews(item, type) {
                    // ç§»é™¤å·²å­˜åœ¨çš„ç›¸åŒé¡?                    this.data.recentViews = this.data.recentViews.filter(view => !(view.id === item.id && view.type === type));
                    // æ·»åŠ åˆ°å¼€å¤?                    this.data.recentViews.unshift({
                        ...item,
                        type,
                        viewedAt: new Date().toISOString()
                    });
                    // ä¿æŒæœ€è¿‘æŸ¥çœ‹ä¸è¶…è¿‡10æ?                    this.data.recentViews = this.data.recentViews.slice(0, 10);
                    return this.data.recentViews;
                }
            };

