                async callAPI(endpoint, data = {}, method = 'POST') {
                    try {
                        // è¿™é‡Œå¯ä»¥æ ¹æ®ä¸åŒçš„endpointè°ƒç”¨ä¸åŒçš„API
                        // ç›®å‰ä½¿ç”¨æ¨¡æ‹Ÿæ•°æ®
