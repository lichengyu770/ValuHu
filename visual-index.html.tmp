                        field.value = field.element.value;
                        field.errors = [];
                        this.updateFieldErrors(formId, fieldName);
                    }
                }
                
                return this;
            }
        };

        // å¤æ‚çš„è·¯ç”±ç³»ç»?        const Router = {
            routes: [],
            currentRoute: null,
            mode: 'hash', // hash or history
            
            // åˆå§‹åŒ–è·¯ç”?            init: function(options = {}) {
                this.mode = options.mode || 'hash';
                
                // ç»‘å®šäº‹ä»¶
                window.addEventListener('popstate', this.handlePopState.bind(this));
                window.addEventListener('hashchange', this.handleHashChange.bind(this));
                
                // åˆå§‹è·¯ç”±
                this.navigate(window.location[this.mode === 'hash' ? 'hash' : 'pathname']);
                
                return this;
            },
            
            // æ·»åŠ è·¯ç”±
            add: function(path, handler) {
                this.routes.push({
                    path: path,
                    handler: handler
                });
                return this;
            },
            
            // å¯¼èˆªåˆ°è·¯ç”?            navigate: function(path) {
                const route = this.matchRoute(path);
                if (route) {
                    this.currentRoute = route;
                    route.handler(route.params);
                    
                    if (this.mode === 'history') {
                        window.history.pushState({ path: path }, '', path);
                    } else {
                        window.location.hash = path;
                    }
                }
                return this;
            },
            
            // åŒ¹é…è·¯ç”±
            matchRoute: function(path) {
                for (const route of this.routes) {
                    const params = this.extractParams(route.path, path);
                    if (params) {
                        return {
                            ...route,
                            params: params
                        };
                    }
                }
                return null;
            },
            
            // æå–å‚æ•°
            extractParams: function(routePath, currentPath) {
                const routeParts = routePath.split('/').filter(part => part);
                const currentParts = currentPath.split('/').filter(part => part);
                
                if (routeParts.length !== currentParts.length) {
                    return null;
                }
                
                const params = {};
                for (let i = 0; i < routeParts.length; i++) {
                    if (routeParts[i].startsWith(':')) {
                        const paramName = routeParts[i].slice(1);
                        params[paramName] = currentParts[i];
                    } else if (routeParts[i] !== currentParts[i]) {
                        return null;
                    }
                }
                
                return params;
            },
            
            // å¤„ç†popstateäº‹ä»¶
            handlePopState: function(event) {
                if (this.mode === 'history' && event.state) {
                    this.navigate(event.state.path);
                }
            },
            
            // å¤„ç†hashchangeäº‹ä»¶
            handleHashChange: function(event) {
                if (this.mode === 'hash') {
                    this.navigate(window.location.hash);
                }
            }
        };

        // æœºå™¨å­¦ä¹ ç®—æ³•æ¨¡å—
        const MachineLearning = {
            // çº¿æ€§å›å½?            linearRegression: {
                // è®­ç»ƒæ¨¡å‹
                train: function(X, y) {
                    const n = X.length;
                    let sumX = 0, sumY = 0, sumXY = 0, sumXX = 0;
                    
                    for (let i = 0; i < n; i++) {
                        sumX += X[i];
                        sumY += y[i];
                        sumXY += X[i] * y[i];
                        sumXX += X[i] * X[i];
                    }
                    
                    const slope = (n * sumXY - sumX * sumY) / (n * sumXX - sumX * sumX);
                    const intercept = (sumY - slope * sumX) / n;
                    
                    return { slope, intercept };
                },
                
                // é¢„æµ‹
                predict: function(model, x) {
                    return model.slope * x + model.intercept;
                }
            },
            
            // é€»è¾‘å›å½’
            logisticRegression: {
                sigmoid: function(z) {
                    return 1 / (1 + Math.exp(-z));
                },
                
                train: function(X, y, learningRate = 0.01, epochs = 1000) {
                    const n = X.length;
                    const features = X[0].length;
                    let weights = new Array(features).fill(0);
                    let bias = 0;
                    
                    for (let epoch = 0; epoch < epochs; epoch++) {
                        let yPred = [];
                        for (let i = 0; i < n; i++) {
                            let z = bias;
                            for (let j = 0; j < features; j++) {
                                z += weights[j] * X[i][j];
                            }
                            yPred[i] = this.sigmoid(z);
                        }
                        
                        let dWeights = new Array(features).fill(0);
                        let dBias = 0;
                        
                        for (let i = 0; i < n; i++) {
                            for (let j = 0; j < features; j++) {
                                dWeights[j] += (yPred[i] - y[i]) * X[i][j];
                            }
                            dBias += (yPred[i] - y[i]);
                        }
                        
                        for (let j = 0; j < features; j++) {
                            weights[j] -= learningRate * dWeights[j] / n;
                        }
                        bias -= learningRate * dBias / n;
                    }
                    
                    return { weights, bias };
                },
                
                predict: function(model, x) {
                    let z = model.bias;
                    for (let i = 0; i < model.weights.length; i++) {
                        z += model.weights[i] * x[i];
                    }
                    return this.sigmoid(z);
                }
            },
            
            // Kæœ€è¿‘é‚»ç®—æ³•
            knn: {
                euclideanDistance: function(a, b) {
                    let sum = 0;
                    for (let i = 0; i < a.length; i++) {
                        sum += Math.pow(a[i] - b[i], 2);
                    }
                    return Math.sqrt(sum);
                },
                
                predict: function(X, y, x, k = 3) {
                    const distances = [];
                    for (let i = 0; i < X.length; i++) {
                        const distance = this.euclideanDistance(X[i], x);
                        distances.push({ distance, label: y[i] });
                    }
                    
                    distances.sort((a, b) => a.distance - b.distance);
                    const neighbors = distances.slice(0, k);
                    
                    const counts = {};
                    neighbors.forEach(neighbor => {
                        counts[neighbor.label] = (counts[neighbor.label] || 0) + 1;
                    });
                    
                    return Object.keys(counts).reduce((a, b) => counts[a] > counts[b] ? a : b);
                }
            },
            
            // å†³ç­–æ ‘ç®—æ³?            decisionTree: {
                // è®¡ç®—åŸºå°¼ç³»æ•°
                giniIndex: function(groups, classes) {
                    let nInstances = 0;
                    for (let group of groups) {
                        nInstances += group.length;
                    }
                    
                    let gini = 0.0;
                    for (let group of groups) {
                        const size = group.length;
                        if (size === 0) continue;
                        
                        let score = 0.0;
                        for (let classVal of classes) {
                            const proportion = group.filter(row => row[row.length - 1] === classVal).length / size;
                            score += proportion * proportion;
                        }
                        gini += (1.0 - score) * (size / nInstances);
                    }
                    return gini;
                },
                
                // åˆ†å‰²æ•°æ®é›?                testSplit: function(index, value, dataset) {
                    const left = [];
                    const right = [];
                    for (let row of dataset) {
                        if (row[index] < value) {
                            left.push(row);
                        } else {
                            right.push(row);
                        }
                    }
                    return [left, right];
